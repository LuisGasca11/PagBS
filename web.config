<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="API Backend" stopProcessing="true">
          <match url="^api/(.*)" />
          <action type="Rewrite" url="http://localhost:3019/api/{R:1}" />
        </rule>
        <rule name="Frontend" stopProcessing="true">
          <match url="(.*)" />
          <action type="Rewrite" url="http://localhost:3304/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
